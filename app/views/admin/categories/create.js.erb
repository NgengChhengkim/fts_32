<% if @category.errors.any? %>
  $(".errors").html("<%= escape_javascript(render 'shared/errors', object: @category) %>")
<% else %>
  $(".category-row").html("<%= escape_javascript(render @categories) %>")
  $(".paginate").html("<%= escape_javascript(render 'shared/paginate') %>")
  $("#form-modal").modal("hide")
<% end %>
